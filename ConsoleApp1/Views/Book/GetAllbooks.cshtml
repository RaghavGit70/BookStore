@{
    ViewData["Title"] = "All books";
}
@model IEnumerable<BookModel>

 <link rel="stylesheet" href="~/CSS/Index.css">
<div class="container">
    <h3 class="display-4">All books</h3>
    <div class="row">

        @foreach (var book in Model)
        {
            <div class="col-md-4">
                <partial name="_bookThumbnail" model="@book" />
            </div>
        }


    </div>
</div>

